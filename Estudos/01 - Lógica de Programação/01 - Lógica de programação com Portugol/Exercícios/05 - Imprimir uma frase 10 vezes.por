programa {
	
	funcao inicio() {

	    /*
	    Objetivo:
	    Imprimir uma frase 10 vezes.
	    Imprima a frase "Eu estou aprendendo laços de repetição" 10 vezes utilizando um laço de repetição.
	    */
	
	    para(inteiro i = 1; i <= 10; i++) {
	      escreva(i, ") Eu estou aprendendo laços de repetição.", "\n")
	    }
	}
}